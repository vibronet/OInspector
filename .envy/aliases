..=cd ..
...=cd ..\..
status=git status
root=cd /d %REPOROOT%
aliases=notepad %REPOROOT%\.envy\aliases
binaries=start %BUILD_OUTPUT%
nuke=git clean -xfd
sln=%REPOROOT%\OInspector.sln
unstage=git reset HEAD $*
gl=git log --all --graph --decorate --format=format:"%C(auto)%h%d %C(bold white)%s%C(reset) %C(dim)%C(white)=> by %an %C(green)(%ar)" $*
deploy=powershell.exe -ExecutionPolicy Unrestricted -File %REPOROOT%\tools\deploy.ps1 $*
jwtdecode=powershell.exe -ExecutionPolicy Unrestricted -File %REPOROOT%\tools\jwt-decode.ps1 $*
mstest=%EXTERNAL%\mstest\mstest.exe $*
jwtcleanpii=%EXTERNAL%\jwt.cleanpii\jwt.cleanpii.exe $*
samples=dir /B %REPOROOT%\Inspector.Tests\testSamples
runtests=%REPOROOT%\tools\runtests.cmd
envy-code=%REPOROOT%\.envy\envy-code.cmd $*